{
  "name": "@repo/composables",
  "version": "1.0.4",
  "type": "module",
  "description": "Vue composables which can be reused across projects.",
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "test": "vitest run",
    "test:dev": "vitest",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "typecheck:lib": "vue-tsc --noEmit -p tsconfig.lib.json",
    "typecheck:vitest": "vue-tsc --noEmit -p tsconfig.vitest.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DesselBane/mono",
    "directory": "packages/composables"
  },
  "exports": {
    "types": "./dist/main.d.ts",
    "import": "./dist/main.js"
  },
  "files": [
    "dist/**",
    "package.json",
    "CHANGELOG.md"
  ],
  "volta": {
    "extends": "../../package.json"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "peerDependencies": {
    "vue": "^3.5.0"
  },
  "devDependencies": {
    "@types/node": "24.0.10",
    "@vitest/coverage-v8": "3.2.4",
    "@repo/configs": "workspace:*",
    "type-fest": "4.41.0",
    "typescript": "5.8.3",
    "vite": "6.3.5",
    "vite-plugin-dts": "4.5.4",
    "vitest": "3.2.4",
    "vue-tsc": "2.2.12"
  },
  "dependencies": {
    "@repo/ts-helpers": "workspace:*"
  }
}
