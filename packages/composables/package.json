{
  "name": "@desselbane/composables",
  "version": "2.0.7",
  "type": "module",
  "description": "Vue composables which can be reused across projects.",
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "test": "vitest run",
    "test:dev": "vitest",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "pnpm lint --fix",
    "typecheck": "vue-tsc --build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DesselBane/mono",
    "directory": "packages/composables"
  },
  "exports": {
    "types": "./dist/main.d.ts",
    "import": "./dist/main.js"
  },
  "files": [
    "dist/**",
    "package.json",
    "CHANGELOG.md"
  ],
  "volta": {
    "extends": "../../package.json"
  },
  "lint-staged": {
    "*": "eslint --fix --max-warnings 0"
  },
  "peerDependencies": {
    "vue": "^3.5.0"
  },
  "devDependencies": {
    "@desselbane/configs": "workspace:*",
    "@desselbane/vitest-helpers": "workspace:*",
    "@types/node": "24.10.4",
    "@vitest/coverage-v8": "4.0.15",
    "type-fest": "5.3.1",
    "typescript": "5.9.3",
    "vite": "7.2.7",
    "vite-plugin-dts": "4.5.4",
    "vitest": "4.0.15",
    "vue-tsc": "3.1.8"
  },
  "dependencies": {
    "@desselbane/ts-helpers": "workspace:*"
  }
}
