{
  "name": "@desselbane/setup",
  "version": "0.0.0",
  "type": "module",
  "description": "Setting up a new environment with Dessel Banes most favourite settings and apps.",
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:dev": "vitest",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "typecheck:lib": "tsc --noEmit -p tsconfig.lib.json",
    "typecheck:vitest": "tsc --noEmit -p tsconfig.vitest.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DesselBane/mono",
    "directory": "packages/setup"
  },
  "exports": {
    "types": "./dist/main.d.ts",
    "import": "./dist/main.js"
  },
  "files": [
    "dist/**",
    "package.json",
    "CHANGELOG.md"
  ],
  "volta": {
    "extends": "../../package.json"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "devDependencies": {
    "@desselbane/configs": "workspace:*",
    "@types/node": "24.0.12",
    "@vitest/coverage-v8": "3.2.4",
    "tsup": "8.5.0",
    "type-fest": "4.41.0",
    "typescript": "5.8.3",
    "vitest": "3.2.4"
  }
}
